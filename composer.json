{
    "name": "lotous/elibom-notification-channel",
    "description": "Elibom Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "elibom"],
    "license": "MIT",
    "authors": [
        {
            "name": "Elbert Tous",
            "email": "contact@lotous.com.co"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "composer/package-versions-deprecated": "^1.8",
        "illuminate/notifications": "~5.8.0|^6.0|^7.0|^8.0",
        "lotous/elibom-laravel": "^1.0.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Lotous\\Elibom\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lotous\\Elibom\\Tests\\Notifications\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Lotous\\Elibom\\Notifications\\ElibomChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
